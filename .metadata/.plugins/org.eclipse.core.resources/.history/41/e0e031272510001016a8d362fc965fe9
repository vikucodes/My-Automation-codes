package arrays;

public class Shift_Zeroes_right {

	public static void main(String[] args) {

		int [] arr= {10,15,0,26,9,1,0,6,0,69,8,0,96,0,6,63,0,12,16,0};
				
		int non_zeroes=0;
		
		for(int i=0;i<arr.length;i++) {
			
			if(arr[i]!=0) {
				
				arr[non_zeroes]=arr[i];
				non_zeroes++;
			}
		}
		for(int i=non_zeroes;i<arr.length;i++) {
			
			non_zeroes=arr[i];
		}
		
		for(int value:arr) {
			
			System.out.println(value);
		}
		
	}

}
