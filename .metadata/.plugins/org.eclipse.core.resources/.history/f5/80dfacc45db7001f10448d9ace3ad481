		//2nd count occurance of apecific int
		
		int []arr= {10,20,10,30,20,40,30,50,80};
		
		int target=20;
		int count=0;
		for(int i=0;i<arr.length;i++) {
			
			if(arr[i]==20) {
				count++;
			}
		}
		
		System.out.println(target + " "+ count);
