package Dailypractice;

public class practice29122024 {

	public static void main(String[] args) {
	
		String name="aman kumar mishra aman kumar mishra aman kumar mishra";
		
		String [] split=name.split(" ");
		
		//String target="aman";
		
		int count_aman=0;
		int count_mishra=0;
		int count_kumar=0;
		
		for(String p:split) {
			
			for(int i=0;i<p.length();i++) {
				
				if(p.contains("aman")||p=="mishra"||p=="kumar") {
				 
					count_aman++;
					count_mishra++;
					count_kumar++;
				
			}
				
		}
			System.out.println(count_kumar);

	}
	}
}


