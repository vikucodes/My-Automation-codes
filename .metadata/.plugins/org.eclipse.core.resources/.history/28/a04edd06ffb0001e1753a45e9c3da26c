package practice;

public class practicejava {

	public static void main(String[] args) {
		int a=7;
		int u=9;
		System.out.println(a>u);
		System.out.println(a!=u);
		System.out.println(a+u);
		
		String s="i love you ";
		System.out.println(s.substring(0,5));
		System.out.println(s.concat("king"));
		System.out.println(s.toUpperCase());

	}
}
