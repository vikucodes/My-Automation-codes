package practice;

import java.util.ArrayList;

public class Arraylist {

	public static void main(String[] args) {
		ArrayList <String>cars=new ArrayList <String>();
		cars.add("volvo");
		cars.add("audi");
		cars.add("BMW");
		cars.add("scorpio");
		//for(String a:cars) {
		System.out.println(cars);
		System.out.println(cars.get(3));//Scorpio
System.out.println(cars.remove(1));//audi
System.out.println(cars.set(0, "abc"));
	}

}
